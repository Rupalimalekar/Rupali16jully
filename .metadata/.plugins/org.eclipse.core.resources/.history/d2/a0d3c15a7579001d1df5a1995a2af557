package logical;

public class MultiplicationWithoutOperator {

	public static void main(String[] args)
	{
       int a=5;
       int b=3;
       int sum=0;
       
//     5+5+5    3+3+3+3+3
       
       for(int i=1;i<=b;i++)
       {
    	   sum=sum+a;
    	//0+5=5
       //5+5=10
      //10+5=15
    	   
       }
		System.out.println(sum);
		

	}

}
